{
  "name": "discord-ses-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r esm ./index.js",
    "keepRunning": "nodemon -r esm ./index.js",
    "test": "ava",
    "lint": "eslint './*.js'",
    "lint:fix": "eslint './*.js' --fix",
    "build": "cp -R xsnap-build/* node_modules/@agoric/xsnap/build/"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "esm"
    ],
    "timeout": "2m"
  },
  "author": "<dan@danfinlay.com>",
  "license": "ISC",
  "repository": "https://github.com/danfinlay/discord-ses-bot.git",
  "dependencies": {
    "@agoric/xsnap": "^0.4.0",
    "discord.js": "^12.5.2",
    "ses": "^0.13.4"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1"
  }
}
